#include <iostream>

using namespace std;

  int main(){
  	int n = 0, largest, count = 0, lag;
  	
  	while(count <=10){
  		
  		cout<<"Digite um numero qualquer: ";
  		cin>>n;
  		count++;
  		
  		if(count == 0){
  			
  			largest = n;	
  		}else{
  			if(n>largest){
  				largest = n;
  				}
			if(count == 1){
				lag = n;
				}		  	
			}
		}
	cout<<"\nO maior numero digitado e: "<<largest<<" com antecessor "<<lag<<endl;
	  
  	return 0;
	}
