#include <iostream>
using namespace std;

#include <iomanip>
using std::setw;

int main(){
	
	int n, i=0, p=0;
	
	cout<<"Me diga o tamanho do quadrado que voce quer: ";
	cin>>n;
	
	while(i<=n){
		
		cout<<"*";
		i++;

		if(i == n){
			
			while(true){
			p+=i;
			cout<<"\n*"<<setw(n-1);
			
			if(p==n){
				cout<<"*****";
				break;
				}
			}
			}
}
return 0;
}
